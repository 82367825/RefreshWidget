<manifest package="com.zero.refreshwidget"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".demo.MainActivity">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER"></category>
                <action android:name="android.intent.action.MAIN"></action>
            </intent-filter>
        </activity>
        <activity android:name=".demo.ListViewDemoActivity">
        </activity>
        <activity android:name=".demo.GridViewDemoActivity">
        </activity>
        <activity android:name=".demo.ScrollViewDemoActivity">
        </activity>
    </application>

</manifest>
